            <owl:Restriction>
                <owl:onProperty rdf:resource="http://webprotege.stanford.edu/RftWPEDTNGrriaufjBA6oG"/>
                <owl:someValuesFrom rdf:resource="http://webprotege.stanford.edu/RCSp3Q6QJjaE6ehKKudHudg"/>
