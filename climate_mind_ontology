    <owl:NamedIndividual rdf:about="http://webprotege.stanford.edu/R9EmN8lJbIyQi0t6oj8e1w4">
        <rdf:type rdf:resource="http://webprotege.stanford.edu/RBpjpLtI6OD4Rbm7grr6kJK"/>
