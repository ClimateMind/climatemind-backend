    <!-- http://webprotege.stanford.edu/R9asiFQ7bVkymcqkQyMOpXn -->

    <owl:NamedIndividual rdf:about="http://webprotege.stanford.edu/R9asiFQ7bVkymcqkQyMOpXn">
        <rdf:type rdf:resource="http://webprotege.stanford.edu/RBpjpLtI6OD4Rbm7grr6kJK"/>
